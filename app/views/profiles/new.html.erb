<h1>Profiles#new</h1>
<p>Find me in app/views/profiles/new.html.erb</p>
<div>
  <%= form_for @profile, html: { multipart: true } do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :attachment %>
    <%= f.file_field :attachment %>
    <%= f.submit "Save" %>
  <% end %>
</div>